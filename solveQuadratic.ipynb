   "source": [
    "def quadratic(a, b, c):\n",
    "    option1 = (-b + pow((b**2 - 4*a*c), 1/2)) / (2*a)\n",
    "    option2 = (-b - pow((b**2 - 4*a*c), 1/2)) / (2*a)\n",
    "    if option1 == option2:\n",
    "        return option1\n",
    "    return option1, option2\n",
    "quadratic(1, 0, -16)"
   ]
